{
    "name": "zendframework/zend-text",
    "description": "Create text based tables and FIGlets",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf",
        "zendframework",
        "text"
    ],
    "support": {
        "docs": "https://docs.zendframework.com/zend-text/",
        "issues": "https://github.com/zendframework/zend-text/issues",
        "source": "https://github.com/zendframework/zend-text",
        "rss": "https://github.com/zendframework/zend-text/releases.atom",
        "chat": "https://zendframework-slack.herokuapp.com",
        "forum": "https://discourse.zendframework.com/c/questions/components"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.6-dev",
            "dev-develop": "2.7-dev"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-stdlib": "^2.7 || ^3.0",
        "zendframework/zend-servicemanager": "^2.7.5 || ^3.0.3"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7 || ^6.4",
        "zendframework/zend-config": "^2.6",
        "zendframework/zend-coding-standard": "~1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Zend\\Text\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZendTest\\Text\\": "test/"
        }
    }
}
